(defun random-elt (choices)
    (nth (random (length choices)) choices))
    
(defun dyads5-graph ()
  (new graph of 
       `(((1 2) :id ab :to ,(random-elt '(cd de ce)))
	 ((1 3) :id ac :to ,(random-elt '(de be bd)))
	 ((1 4) :id ad :to ,(random-elt '(be bc ce)))
	 ((1 5) :id ae :to ,(random-elt '(cd bd bc)))
	 ((2 3) :id bc :to ,(random-elt '(de ae ad)))
	 ((2 4) :id bd :to ,(random-elt '(ce ae ac)))
	 ((2 5) :id be :to ,(random-elt '(cd ad ac)))
	 ((3 4) :id cd :to ,(random-elt '(ab ae be)))
	 ((3 5) :id ce :to ,(random-elt '(ab bd ad)))
	 ((4 5) :id de :to ,(random-elt '(bc de ab))))))

